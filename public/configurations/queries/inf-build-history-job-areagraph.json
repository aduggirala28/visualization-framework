{
    "id": "inf-build-history-job-areagraph",
    "title": "Builds per day",
    "service": "elasticsearch",
    "query": {
        "index": "{{index:nuage_build}}",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "size": 0,
            "query": {
                "bool": {
                    "must": [
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-7d}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        },
                        {
                            "term":{
                                "job":  "{{job}}"
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "ts": {
                    "date_histogram": {
                        "field": "timestamp",
                        "interval":"1d",
                        "extended_bounds":{
                            "min":"{{startTime:now-7d}}",
                            "max":"{{endTime:now}}"
                        }
                    },
                    "aggs": {
                        "count": {
                          "terms": {
                              "field": "result"
                          }
                        }
                    }
                }
            }
        }
    }
}