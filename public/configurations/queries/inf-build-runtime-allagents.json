{
    "id": "inf-build-runtime-allagents",
    "title": "Total build during the week for per agent",
    "service": "elasticsearch",
    "query": {
        "index": "{{index:nuage_build}}",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "size": 0,
            "query": {
                "bool": {
                    "must": [
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-7d}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "agent": {
                    "terms": {
                        "field": "agent",
                        "size": 50
                    },
                    "aggs":{
                        "runtime":{
                            "sum":{
                                "field": "duration"
                            }
                        }
                    }
                }
            }
        }
    }
}