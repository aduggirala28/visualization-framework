{
    "id": "inf-build-runtime-stack",
    "title": "Builds per day",
    "service": "elasticsearch",
    "query": {
        "index": "{{index:nuage_build}}",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "size": 0,
            "query": {
                "bool": {
                    "must_not": [
                    ],
                    "must": [
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-7d}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "job": {
                    "terms": {
                        "field": "job",
                        "size": 1000
                    },
                    "aggs": {
                        "runtime": {
                          "sum": {
                              "field": "duration"
                          }
                        }
                    }
                }
            }
        }
    }
}