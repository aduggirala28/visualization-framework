{
    "id": "inf-build-projects",
    "title": "All projects in builds",
    "service": "elasticsearch",
    "query": {
        "index": "{{index:nuage_build}}",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "size": 0,
            "query": {
                "bool": {
                    "must_not": [
                    ],
                    "must": [
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-24h}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "project": {
                    "terms": {
                        "field": "project",
                        "size":"50",
                        "order":{
                            "count":"desc"
                        }
                    }
                }
            }
        }
    }
}