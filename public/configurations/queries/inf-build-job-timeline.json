{
    "id": "inf-build-job-timeline",
    "title": "Job and its duration for the given week/day",
    "service": "elasticsearch",
    "query": {
        "index": "{{index:nuage_build}}",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "sort":{
                "timestamp":"asc"
            },
            "query": {
                "bool": {
                    "must": [
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-7d}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        },
                        {
                            "term":{
                                "job":  "{{job}}"
                            }
                        }
                    ]
                }
            }
        }
    }
}